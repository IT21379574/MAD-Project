<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Order">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="95dp"
        android:layout_height="58dp"
        app:srcCompat="@drawable/back_arrow"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="32dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/Promo_code"
        android:layout_width="159dp"
        android:layout_height="40dp"
        android:ems="10"
        android:hint="@string/Promo_code"
        android:inputType="textPersonName"
        android:padding="12dp"
        android:textColor="#707070"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="223dp"
        tools:layout_editor_absoluteY="560dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tvNewPrice"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="228dp"
        android:layout_marginTop="660dp"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/welcome4"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="660dp"
        android:text="Total:"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvPrice"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="224dp"
        android:layout_marginTop="392dp"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/welcome2"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="392dp"
        android:text="@string/Order_total"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/procced"
        style="@android:style/Widget.Button"
        android:layout_width="251dp"
        android:layout_height="62dp"
        android:text="@string/procced"
        android:textColor="#ffffff"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="714dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/apply"
        style="@android:style/Widget.Button"
        android:layout_width="121dp"
        android:layout_height="43dp"
        android:backgroundTint="#FFFFFF"
        android:text="@string/apply"
        android:textColor="#ffffff"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="261dp"
        tools:layout_editor_absoluteY="613dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="169dp"
        android:layout_height="51dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="102dp"
        android:text="@string/oder"
        android:textAlignment="textStart"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/welcome"
        android:layout_width="284dp"
        android:layout_height="68dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="108dp"
        android:text="@string/Order_details"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="202dp"
        android:layout_height="125dp"
        app:srcCompat="@drawable/download__2_"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="176dp"
        tools:ignore="MissingConstraints" />

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="237dp"
        android:layout_height="49dp"
        android:rating="3.5"
        android:scrollbarSize="1dp"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="320dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="76dp"
        android:layout_height="44dp"
        android:text="3.5"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="291dp"
        tools:layout_editor_absoluteY="322dp"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="274dp"
        android:layout_height="53dp"
        android:checked="false"
        android:text="@string/oder_req01"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="446dp"
        tools:ignore="MissingConstraints" />

    <CheckBox
        android:id="@+id/checkBox2"
        android:layout_width="213dp"
        android:layout_height="44dp"
        android:checked="false"
        android:text="@string/oder_rq02"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="509dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="147dp"
        android:layout_height="53dp"
        android:text="@string/oder_discount"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="560dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>