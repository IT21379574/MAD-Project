<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OrderSummery">

    <ImageView
        android:id="@+id/odersumbackbtn"
        android:layout_width="90dp"
        android:layout_height="51dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.074"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_arrow"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/welcome14"
        android:layout_width="284dp"
        android:layout_height="68dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="87dp"
        android:text="@string/Order_details"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/signup3"
        style="@android:style/Widget.Button"
        android:layout_width="289dp"
        android:layout_height="81dp"
        android:layout_marginBottom="52dp"
        android:backgroundTint="#FFFFFF"
        android:text="@string/checkout"
        android:textColor="#ffffff"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome6"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/welcome6"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="63dp"
        android:layout_marginTop="71dp"
        android:text="105$"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/welcome8"
        app:layout_constraintTop_toBottomOf="@+id/welcome12" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="38dp"
        android:layout_marginEnd="35dp"
        android:layout_marginBottom="79dp"
        android:text="Add high quality files"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/welcome8"
        app:layout_constraintEnd_toStartOf="@+id/welcome12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/welcome12"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="15dp"
        android:text="+30$"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/welcome7" />

    <TextView
        android:id="@+id/welcome10"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="39dp"
        android:text="50$"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome14" />

    <TextView
        android:id="@+id/welcome9"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="59dp"
        android:text="Order Total:"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toStartOf="@+id/welcome10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome14" />

    <TextView
        android:id="@+id/welcome8"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="79dp"
        android:text="Total:"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="75dp"
        android:text="One day dilvery"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/welcome7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome9"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/welcome7"
        android:layout_width="137dp"
        android:layout_height="41dp"
        android:layout_marginStart="236dp"
        android:layout_marginTop="11dp"
        android:text="+25$"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcome10" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="236dp"
        android:layout_height="46dp"
        android:layout_marginTop="109dp"
        android:text="Summary"
        android:textAlignment="center"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>